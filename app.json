{
  "expo": {
    "name": "ta_monitor_air",
    "plugins": ["@react-native-firebase/app", "@react-native-firebase/perf"],
    "displayName": "EWS Monitor Air",
    "slug": "ta_monitor_air",
    "version": "0.8.2",
    "orientation": "portrait",
    "icon": "./app/assets/appicon_purabaya.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logo_purabaya.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.ewsmonitorair"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
